algoritmo "semnome"
// Função :
// Autor :
// Data : 15/08/2022
// Seção de Declarações 
var
   dataNascimento,anoAtual, idade: inteiro
   
inicio
   escreva("digite o ano atual: ")
   leia(anoAtual)
   escreva("digite sua data de nascimento: ")
   leia(dataNascimento)
   idade <- anoAtual  - datanascimento
   escreva("sua idade é: ", idade, " anos")
   

fimalgoritmo
