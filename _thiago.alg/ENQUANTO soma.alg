algoritmo "soma dos números lidos"
// Função : ENQUANTO
// Autor : RAPOSO
// Data : 27/09/2022
// Seção de Declarações 
var
  i, num, soma: inteiro
inicio
   i <- 1
   soma <- 0
   enquanto (i<=10) faca
      escreva("digite um número: ")
      leia(num)
      soma <- soma + num
      i <- i+1
   fimenquanto
   escreva("a soma dos numeros é: ", soma)
fimalgoritmo
