algoritmo "repetção de números iímpares "
// Função : repita
// Autor : tigs
// Data : 08/09/2022
// Seção de Declarações
var
num, a, i: inteiro
inicio
   escreva("REPETIÇÃO DE NÚMEROS IÍMPARES")
   escreval
   i <- 1
   repita
      escreva(i) //número recebido
      i <- i+2
   ate i > 50 // limite de repetções

fimalgoritmo
